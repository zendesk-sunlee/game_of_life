<h1>Generations#show</h1>
<p>Find me in app/views/generations/show.html.erb</p>

<%= @game %>

<table>
<tbody>
<% for x in 1..10 do %>
  <tr>
    <% for y in 1..10 do %>
      <td style="background-color: <%= @game.include?(Cell.new(x, y)) ? 'red' : 'grey' %>; width: 15px; height: 15px;">
      </td>
    <% end %>
  </tr>
<% end %>
</tbody>
</table>